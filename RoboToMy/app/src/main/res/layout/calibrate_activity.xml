    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:gravity="start"
    android:baselineAligned="true"
    android:id="@+id/ER">
<ListView
    android:layout_width="fill_parent"
    android:layout_height="500dp"
    android:orientation="vertical"
    android:id="@+id/calibrationList"
